
// 110 - Hacer un pseudocódigo que imprima la suma de los números del 1 al 100

Algoritmo ejercicio110
	
	lasuma = 0
	
	Para contador <- 1 Hasta 100 Con Paso 1 Hacer
		lasuma = lasuma + contador
	Fin Para
	
	Escribir "El resultado de sumar los 100 numeros es: ", lasuma
	
FinAlgoritmo
