
Algoritmo sin_titulo
	
	Escribir "Ingrese un numero:"
	Leer numero1
	
	Escribir "Ingrese el segundo numero:"
	Leer numero2
	
	lasuma = numero1 + numero2
	
	laresta = numero1 - numero2
	
	elproducto = numero1 * numero2
	
	ladivision = numero1 / numero2
	
	elmodulo = numero1 % numero2
	
	Escribir "Suma: ", lasuma
	Escribir "Resta: ", laresta
	Escribir "Producto: ", elproducto
	Escribir "Division: ", ladivision
	Escribir "Modulo: ", elmodulo
	
FinAlgoritmo
